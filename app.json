{
  "expo": {
    "name": "araname",
    "slug": "araname",
    "version": "1.1.3",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-icon.png",
        "tabletImage": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#CCDDCC"
      },
      "bundleIdentifier": "dev.keiver.araname",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save images and videos.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save downloaded media to your photo library."
      }
    },
    "android": {
      "package": "dev.keiver.araname",
      "permissions": ["INTERNET", "WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"],
      "usesCleartextTraffic": true
    },
    "plugins": [
      "expo-asset",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#CCDDCC",
          "tabletImage": "./assets/splash-icon.png"
        }
      ],
      [
        "expo-file-system",
        {
          "filePermissions": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow araname to access your photos to save images and videos.",
          "savePhotosPermission": "Allow araname to save images and videos to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-6899536859351708~1684804101",
          "iosAppId": "ca-app-pub-6899536859351708~7051647011"
        }
      ],
      "react-native-iap"
    ]
  }
}
